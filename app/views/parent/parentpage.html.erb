<div id="anounce" class="anouncements">
            <h1>ANOUNCEMENTS</h1>                    
    <div>
        <% @post.each do |post| %>  
            <div class="post">
                <h2><%= post.title %></h2>
                <p class="date"><%= post.created_at.strftime("%a %l:%M%P %b %d, %Y")%></p>
                <div><%= post.body %></div>    
            </div>
        <% end %>
    </div>  
</div>




            
        
